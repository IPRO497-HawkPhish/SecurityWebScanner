{
  "manifest_version": 3,
    "name": "HawkPhish",
    "description": "Web Scanner and Security Summarizer",
    "version": "1.0",
    "permissions": ["activeTab"],

    "icons": {
      "16": "assets/icons/logo-192.png"
    },

    "action": {
      "default_popup": "popup/popup.html"
    },

    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"]
      }
    ]
}
