{
  "manifest_version": 3,
    "name": "HawkPhish",
    "description": "Web Scanner and Security Summarizer",
    "version": "2.0",
    "permissions": ["activeTab"],

    "icons": {
      "16": "popup/logo/phish.png",
      "48": "popup/logo/phish.png",
      "128": "popup/logo/phish.png"
    },

    "action": {
      "default_popup": "popup/popup.html"
    },

    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"],
        "css": ["assets/css/main.css"]
      }
    ]
}
